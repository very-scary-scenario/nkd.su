    "test": "pytest"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "less": "^4.1.2",
    "uglify-js": "^3.14.2",
    "yuglify": "^2.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "details-polyfill": "^1.2.0",
    "jsmediatags": "^3.9.7",
    "sortablejs": "^1.14.0"
  }
}
