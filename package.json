{
  "name": "nkd.su",
  "version": "0.0.0",
  "scripts": {
    "check-migrations": "python manage.py makemigrations --dry-run --check",
    "lint-black": "black --check --diff .",
    "lint-flake8": "flake8 .",
    "lint-js": "eslint nkdsu/static/js",
    "lint-mypy": "mypy --pretty --show-error-context",
